@article{Kantartzis2002,
abstract = {A new higher order finite-difference time-domain (FDTD) methodology for the consistent modeling of arbitrarily shaped antennas in three-dimensional (3-D) curvilinear coordinates is presented in this paper. The generalized algorithm, which introduces novel conventional and nonstandard regimes, develops advanced PMLs and compact differences to handle the widened spatial increments. Also, a systematic leapfrog integrator with mesh expansion concepts is established. Beyond diverse 3-D structures, analysis studies fractal arrays whose self-similarity renders them ideal for small-sized designs. Results indicate that the proposed method achieves a critical elimination of lattice errors and provides very precise radiation patterns.},
author = {Kantartzis, Nikolaos V. and Zygiridis, Theodoros T. and Tsiboukis, Theodoros D.},
doi = {10.1109/20.996191},
file = {:C\:/Users/karpick/Desktop/Лит-ра для диплома/kantartzis2002.pdf:pdf},
issn = {00189464},
journal = {IEEE Transactions on Magnetics},
keywords = {Antennas,Fractals,General curvilinear coordinates,Higher order FDTD methods,Numerical methods},
number = {2 I},
pages = {737--740},
title = {{A nonstandard higher order FDTD algorithm for 3-D arbitrarily and fractal-shaped antenna structures on general curvilinear lattices}},
volume = {38},
year = {2002}
}
@article{Taniguchi2008,
abstract = {We have shown that a perfectly conducting wire has an equivalent radius, $\alpha$0 = 0.135$\Delta$ r ($\Delta$ r is the lateral side length of rectangular cells used), if the tangential components of electric field along the wire axis are forced to zero in finite-difference time-domain (FDTD) calculations in the 2-D cylindrical coordinate system. Further, we have shown that the technique proposed by Noda and Yokoyama to represent a wire having an arbitrary radius in the 3-D Cartesian coordinate system can be applied successfully to representing such a wire in the 2-D cylindrical coordinate system if 0.135 $\Delta$ r is used instead of 0.230 $\Delta$ r for $\alpha$0. {\textcopyright} 2008 IEEE.},
author = {Taniguchi, Y. and Baba, Y. and Nagaoka, N. and Ametani, A.},
doi = {10.1109/TEMC.2008.2004466},
file = {:C\:/Users/karpick/Desktop/Лит-ра для диплома/taniguchi2008.pdf:pdf},
issn = {00189375},
journal = {IEEE Transactions on Electromagnetic Compatibility},
keywords = {2-D cylindrical coordinate system,Electromagnetic field,Finite-difference time-domain FDTD) method,Thin wire},
number = {4},
pages = {1014--1018},
title = {{Representation of an arbitrary-radius wire for FDTD calculations in the 2-D cylindrical coordinate system}},
volume = {50},
year = {2008}
}
